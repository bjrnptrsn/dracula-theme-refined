{
	"name": "theme-dracula-refined",
	"version": "2.24.3",
	"displayName": "Dracula Refined",
	"homepage": "https://github.com/mathcale/dracula-theme-refined",
	"description": "An attempt to improve Dracula colors consistency on VS Code, making it more pleasant to the eyes during coding sessions",
	"publisher": "mathcale",
	"license": "MIT",
	"scripts": {
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"package": "vsce package -o ./bin/dracula.vsix",
		"vscode:prepublish": "npm run build",
		"vsce-publish": "vsce publish"
	},
	"maintainers": [
		"Derek P Sifford <dereksifford@gmail.com>",
		"Matheus Calegaro <matheuscalegaro@outlook.com>"
	],
	"contributors": [
		"Krzysztof Wisznarewski",
		"Daniel Ramos",
		"Eric Jackson",
		"Elliott Pogue",
		"Ajit Singh",
		"Teddy Bradford"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/mathcale/dracula-theme-refined.git"
	},
	"bugs": {
		"url": "https://github.com/mathcale/dracula-theme-refined/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dracula",
		"dracula-refined",
		"dark",
		"theme",
		"color-theme"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#3c4557",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Dracula Refined",
				"uiTheme": "vs-dark",
				"path": "./theme/dracula.json"
			}
		]
	},
	"devDependencies": {
		"js-yaml": "^4.1.0",
		"prettier": "^2.6.0",
		"tinycolor2": "^1.4.2",
		"vsce": "^2.7.0"
	}
}
